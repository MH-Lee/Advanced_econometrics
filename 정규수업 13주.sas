DATA ip;
	INFILE 'c:\data\ip.prn';
	INPUT mon ip;
	ipg = DIF(LOG(ip))*100;
	IF mon < 19590101 THEN DELETE;
RUN;
DATA fyff;
	INFILE 'c:\data\fyff.prn';
	INPUT mon fyff;
	fyff4 = LAG4(fyff);
	IF mon < 20080101 THEN DO;
		int1 = 1;
		int2 = 0;
		fyff41 = fyff;
		fyff42 = 0;
	END;
	ELSE DO;
		int1 = 0;
		int2 = 1;
		fyff41 = 0;
		fyff42 = fyff4;
	END;
IF mon < 19590101 THEN DELETE;
RUN;
DATA all;
	MERGE ip fyff;
	BY mon;
RUN;
PROC AUTOREG DATA = all;
	MODEL ipg = int1 int2 fyff41 fyff42/NOINT NLAG = 1 METHOD = ml MAXIT = 200;
	TEST  int1 = int2,  fyff41 = fyff42;
RUN;
PROC AUTOREG DATA = all;
	MODEL ipg = int1 int2 fyff41 fyff42/NOINT NLAG = 1 METHOD = ml MAXIT = 200;
	TEST  int1 = int2,  fyff41 = fyff42/TYPE = wald;
RUN;
